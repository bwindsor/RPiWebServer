{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "protocol": "legacy",
            "name": "Launch Web Server",
            "program": "${workspaceRoot}/bin/climateserver.js",
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to node Process",
            "processId": "${command:PickProcess}",
            "port": 5858
        }
    ]
}